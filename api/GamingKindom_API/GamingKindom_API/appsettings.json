{
  "NLog": {
    "throwConfigExceptions": true,
    "targets": {
      "logfile": {
        "type": "File",
        "fileName": "logs/logfile.txt",
        "layout": "${longdate} ${level:uppercase=true} ${message} ${exception:format=toString,StackTrace}"
      },
      "logconsole": {
        "type": "Console",
        "layout": "${longdate} ${level:uppercase=true} ${message} ${exception:format=toString,StackTrace}"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "logfile,logconsole"
      }
    ]
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=GamingKingdomDatabase;Database=GamingKingdomDB;User Id=sa;Password=MyTopSecretPassword123;TrustServerCertificate=True;MultipleActiveResultSets=true"
  }
}
